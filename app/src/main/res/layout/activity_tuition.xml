<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".View.Tuition">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="30dp"
        android:autoSizeTextType="uniform"
        android:text="$"
        app:layout_constraintStart_toEndOf="@+id/CSFee"
        app:layout_constraintTop_toBottomOf="@+id/textView11" />

    <TextView
        android:id="@+id/textView17"
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="41dp"
        android:autoSizeTextType="uniform"
        android:text="$"
        app:layout_constraintStart_toEndOf="@+id/StatFee"
        app:layout_constraintTop_toBottomOf="@+id/textView13" />

    <TextView
        android:id="@+id/textView18"
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="72dp"
        android:autoSizeTextType="uniform"
        android:text="$"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/totalFee"
        app:layout_constraintTop_toBottomOf="@+id/textView17" />

    <TextView
        android:id="@+id/textView21"
        android:layout_width="64dp"
        android:layout_height="28dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        android:text="Satistics"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView20" />

    <TextView
        android:id="@+id/textView22"
        android:layout_width="70dp"
        android:layout_height="31dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="88dp"
        android:autoSizeTextType="uniform"
        android:text="Total"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView21" />

    <TextView
        android:id="@+id/textView23"
        android:layout_width="64dp"
        android:layout_height="28dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        android:text="Chiness"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/TuitionHeader" />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="82dp"
        android:layout_height="50dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="36dp"
        android:text="Computer Science"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView23" />

    <TextView
        android:id="@+id/StatFee"
        android:layout_width="156dp"
        android:layout_height="44dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="36dp"
        android:autoSizeTextType="uniform"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/textView21"
        app:layout_constraintTop_toBottomOf="@+id/CSFee" />

    <TextView
        android:id="@+id/chineseFee"
        android:layout_width="156dp"
        android:layout_height="44dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="96dp"
        android:autoSizeTextType="uniform"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/textView23"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/TuitionHeader"
        android:layout_width="129dp"
        android:layout_height="41dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="29dp"
        android:text="Tuition Fee"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/totalFee"
        android:layout_width="156dp"
        android:layout_height="44dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="76dp"
        android:autoSizeTextType="uniform"
        android:text="0"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/textView22"
        app:layout_constraintTop_toBottomOf="@+id/StatFee" />

    <TextView
        android:id="@+id/CSFee"
        android:layout_width="156dp"
        android:layout_height="44dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="28dp"
        android:autoSizeTextType="uniform"
        android:text="0"
        app:layout_constraintStart_toEndOf="@+id/textView20"
        app:layout_constraintTop_toBottomOf="@+id/chineseFee" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="37dp"
        android:layout_height="43dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="99dp"
        android:autoSizeTextType="uniform"
        android:text="$"
        app:layout_constraintStart_toEndOf="@+id/chineseFee"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>